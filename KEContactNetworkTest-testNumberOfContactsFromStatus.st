'From Pharo6.0 of 13 May 2016 [Latest update: #60528] on 3 August 2018 at 10:27:46.282459 pm'!!KEContactNetworkTest methodsFor: 'tests' stamp: 'nguyentienthanh 8/1/2018 09:32'!testNumberOfContactsFromStatus	"self new testNumberOfContactsFromStatus"	|model network sirConcern spatialConcern|	model := KEModel new population: (KEPopulation size: 10).		spatialConcern := KEModelPart new.	network := KEContactNetwork nodes: 10 topology: { #random. #p->1 }.	spatialConcern addParameter: #network value: network.	spatialConcern addAttribute: #node value: network allContacts.		sirConcern := KEModelPart new.	sirConcern addAttribute: #status value: #(S I R).   model integrate: sirConcern.	model integrate: spatialConcern.	1 to: 7 do: [:i| model atCompartment: {#status->#S. #node->i asString asSymbol} put: 1].	model atCompartment: { #status->#I. #node->#'10' } put: 1.	model atCompartment: { #status->#I. #node->#'9' } put: 1.	model atCompartment: { #status->#R. #node->#'8' } put: 1.	self assert: (network contactsFrom: {model. #I}) equals: 18.												self assert: (network contactsFrom: {model. #R}) equals: 9.	self assert: (network contactsFrom: {model. #S}) equals: 63.		! !